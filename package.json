{
  "name": "@francescozoccheddu/ts-goodies",
  "description": "Random TypeScript utilities",
  "author": "Francesco Zoccheddu",
  "url": "https://github.com/francescozoccheddu/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/francescozoccheddu/ts-goodies.git"
  },
  "bugs": {
    "url": "https://github.com/francescozoccheddu/ts-goodies/issues"
  },
  "license": "MIT",
  "version": "0.0.4",
  "keywords": [
    "typescript",
    "utils",
    "collections",
    "types"
  ],
  "scripts": {
    "build": "tsc && cpy types/*.d.ts ../dist/types --cwd src && mk-package",
    "linto": "eslint ./src",
    "clean": "rimraf ./tsconfig.tsbuildinfo && rimraf ./dist",
    "publish": "npm run clean && npm run lint && npm run build && cd ./dist && npm publish --access public"
  },
  "homepage": "https://github.com/francescozoccheddu/ts-goodies#readme",
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "cpx2": "^7.0.1",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "mk-package": "^1.0.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "kleur": "^4.1.5"
  }
}